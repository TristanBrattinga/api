<section class="sidebar">
    <div>
        <a href="/rooms"><img src="/images/caretleft.svg" alt="">Back</a>
    </div>
    <ul>
        <li>
            <img src="/images/box.svg" alt="" />
            <span>Users in the room</span>
        </li>
        <li>
            <img src="/images/box.svg" alt="" />
            <span>Users in the room</span>
        </li>
        <li>
            <img src="/images/box.svg" alt="" />
            <span>Users in the room</span>
        </li>
        <li>
            <img src="/images/box.svg" alt="" />
            <span>Users in the room</span>
        </li>
    </ul>
    <ul id="userList">
        <% room.users.map((user) => { %>
            <li>
                <%= user %>
            </li>
        <% }) %>
    </ul>
    <button id="foldSidebar">
        <img src="/images/arrow.svg" alt="Fold sidebar" />
    </button>
    <div>
        <ul>
            <li>
                <img src="/images/gear.svg" alt="" />
                <span>Support</span>
            </li>
            <li>
                <img src="/images/gear.svg" alt="" />
                <span>Settings</span>
            </li>
        </ul>
        <div class="divider"></div>
        <a id="toProfile" href="/profile">
            <img class="avatar" src="/images/avatar.svg" alt="Avatar image" />
            <div>
                <h2><%= loggedInUser.username %></h2>
                <p><%= loggedInUser.email %></p>
            </div>
        </a>
    </div>
</section>